<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:DbExport.Gui.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="250" d:DesignHeight="500"
             x:Class="DbExport.Gui.Views.SidebarView"
             x:DataType="vm:SidebarViewModel"
             Width="250">

  <Design.DataContext>
    <vm:SidebarViewModel/>
  </Design.DataContext>
  
  <Border>
    <Border.Background>
      <ImageBrush Source="/Assets/banner.jpg" Stretch="Fill" />
    </Border.Background>
  </Border>
</UserControl>
